# Get your key at https://aistudio.google.com/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Supabase (https://supabase.com/dashboard project → Settings → API)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
# Required for signup (create users row), add-user, auth sync. Never expose to client.
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
# Server-only: for syncing auth→USERS and adding users (owner). Never expose to client.
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here
# Fallback user_id for evaluate/run when not logged in (optional if using auth)
DEFAULT_USER_ID=your_default_user_uuid
